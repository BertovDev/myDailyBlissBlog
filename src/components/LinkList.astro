---
import { getCollection } from "astro:content";
import { type CollectionEntry } from "astro:content";
import LinkItem from "./LinkItem.astro";

let links = await getCollection("linkPage");

type LinkCollectionEntry = CollectionEntry<"linkPage">;
---

{
  links.map((el) => {
    return <LinkItem slug={""} {...el.data} />;
  })
}
